{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "@/components/*":["./src/components/*"],
      "@/views/*":["./src/views/*"],
      "@/images/*":["./src/images/*"],
      "@/css/*":["./src/css/*"],
      "@/utils/*":["./src/css/*"],
      "@/routes/*":["./src/routes/*"],
      "@/network/*":["./src/network/*"],
      "@/type/*":["./src/type/*"]
    }
  },
  "include": [
    "src"
  ]
}
